{
  "extends": "airbnb-base",
  "env": {
    "es6": true,
    "browser": true,
    "jest": true
  },
  "rules": {
    "quotes": ["error", "double"],
    "default-case": "error",
    "no-restricted-syntax": ["error", "LabeledStatement", "WithStatement"],
    "no-plusplus": ["error", { "allowForLoopAfterthoughts": true }],
    "no-param-reassign": ["error", { "props": false }],
    "consistent-return": ["error", { "treatUndefinedAsUnspecified": false }]
  }
}
